[alias]
	co = checkout
	ci = commit
	st = status
    br = branch
    cltags = !git tag -d $(git tag) && git fetch --tags
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	bye = !git pull --rebase && git push
